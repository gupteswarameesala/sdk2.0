{
  "appName": "sample-app-python-basic",
  "appVersion": "1.0.0",
  "discoveryConfiguration": [
    {
      "propertyName": "ipAddress",
      "label": "IP Address",
      "dataType": "string",
      "placeholder": "Enter IP Address or hostname"
    },
    {
      "propertyName": "port",
      "label": "Port",
      "dataType": "string"
    },
    {
      "propertyName": "protocol",
      "label": "Protocol",
      "dataType": "string"
    },
    {
      "propertyName": "vcenterName",
      "label": "vcenter name",
      "dataType": "string"
    },
    {
      "propertyName": "credentials",
      "label": "Credentials",
      "dataType": "string",
      "credentialType": "APPLICATION",
      "placeholder": "Select Credentials"
    },
    {
      "propertyName": "notificationAlert",
      "label": "Notification Alert",
      "dataType": "boolean"
    },
    {
      "propertyName": "alertConfiguration",
      "label": "Event polling",
      "dataType": "boolean"
    },
    {
      "propertyName": "alertSeverity",
      "label": "Alert Severity",
      "dataType": "string"
    },
    {
      "propertyName": "alertOnRootResource",
      "label": "Alert on Root Resource",
      "dataType": "boolean"
    },
    {
      "propertyName": "alertSeverityMapping",
      "label": "Alert Severity Mapping",
      "dataType": "string"
    }
  ],
  "required": [
    "ipAddress",
    "port",
    "protocol",
    "vcenterName"
  ],
  "data": {
    "notificationAlert": false
  },
  "nativeTypeMapping": [
    {
      "nativeType": "vcenter",
      "opsrampResourceType": "server"
    },
    {
      "nativeType": "host",
      "opsrampResourceType": "server"
    },
    {
      "nativeType": "vm",
      "opsrampResourceType": "server"
    }
  ],
  "nativeType": [
    {
      "name": "vcenter",
      "metric": [
        {
          "name": "dell_demo_resource_event_statistics",
          "units": "s",
          "alertOn": "STATIC",
          "factor": 1,
          "dataPoint": "Gauge",
          "alertSummary": "dell_demo_resource_event_statistics",
          "alertDescription": "dell_demo_resource_event_statistics",
          "warning": {
            "operator": "GREATER_THAN",
            "value": "50",
            "repeat": 1
          },
          "critical": {
            "operator": "GREATER_THAN",
            "value": "70",
            "repeat": 2
          },
          "availability": true,
          "graphPoint": true,
          "raiseAlert": true,
          "formatPlottedValue": false,
          "formatGraph": false,
          "formatAlert": false
        }
      ],
      "monitors": [
        {
          "title": "Event Statistics Monitor",
          "frequency": 5,
          "metric": [
            "dell_demo_resource_event_statistics"
          ]
        }
      ]
    },
    {
      "name": "host",
      "metric": [
        {
          "name": "dell_demo_host_system_network_interface_utilization",
          "units": "%",
          "alertOn": "STATIC",
          "factor": 1,
          "dataPoint": "Gauge",
          "alertSummary": "dell_demo_host_system_network_interface_utilization",
          "alertDescription": "dell_demo_host_system_network_interface_utilization",
          "warning": {
            "operator": "GREATER_THAN",
            "value": "50",
            "repeat": 1
          },
          "critical": {
            "operator": "GREATER_THAN",
            "value": "70",
            "repeat": 2
          },
          "availability": true,
          "graphPoint": true,
          "raiseAlert": true,
          "formatPlottedValue": false,
          "formatGraph": false,
          "formatAlert": false
        }
      ],
      "monitors": [
        {
          "title": "HOST Performance Monitor",
          "frequency": 5,
          "metric": [
            "dell_demo_host_system_network_interface_utilization"
          ]
        }
      ]
    },
    {
      "name": "vm",
      "metric": [
        {
          "name": "dell_demo_vm_system_cpu_usage_stats",
          "units": "%",
          "alertOn": "STATIC",
          "factor": 1,
          "dataPoint": "Gauge",
          "alertSummary": "dell_demo_vm_system_cpu_usage_stats",
          "alertDescription": "dell_demo_vm_system_cpu_usage_stats",
          "warning": {
            "operator": "GREATER_THAN",
            "value": "50",
            "repeat": 1
          },
          "critical": {
            "operator": "GREATER_THAN",
            "value": "70",
            "repeat": 2
          },
          "availability": true,
          "graphPoint": true,
          "raiseAlert": true,
          "formatPlottedValue": false,
          "formatGraph": false,
          "formatAlert": false
        }
      ],
      "monitors": [
        {
          "title": "VM Performance Monitor",
          "frequency": 5,
          "metric": [
            "dell_demo_vm_system_cpu_usage_stats"
          ]
        }
      ]
    }
  ]
}